{
	"name": "eci-segmenter",
	"version": "1.0.0",
	"description": "Deterministic voter segmentation engine",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc -p tsconfig.json",
		"build:ui": "cd src/ui && npm install && npm run build",
		"build:all": "npm run build && npm run build:ui",
		"start": "node dist/index.js",
		"test": "jest --runInBand",
		"railway:build": "npm install && npm run build && cd src/ui && npm install && npm run build",
		"railway:start": "npm start"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"dependencies": {
		"@supabase/supabase-js": "^2.49.1",
		"@turf/turf": "^7.2.0",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"pg": "^8.12.0",
		"pino": "^9.2.0",
		"puppeteer": "^23.0.0",
		"uuid": "^10.0.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.14.2",
		"@types/pg": "^8.16.0",
		"@types/supertest": "^2.0.16",
		"@types/uuid": "^10.0.0",
		"jest": "^29.7.0",
		"supertest": "^7.0.0",
		"testcontainers": "^10.10.3",
		"ts-jest": "^29.2.5",
		"tsx": "^4.16.2",
		"typescript": "^5.5.4"
	}
}
